{
  "name": "mqtts",
  "version": "0.1.18",
  "description": "MQTT client in Typescript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Nerixyz/mqtts"
  },
  "bugs": {
    "url": "https://github.com/Nerixyz/mqtts/issues"
  },
  "scripts": {
    "test": "jest",
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "prepare": "npm run build",
    "lint": "eslint --fix --ext .js,.ts src/**",
    "format": "prettier --write \"src/**/*.ts\" --loglevel warn",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "precommit": "npm run typecheck && npm run lint && npm run format"
  },
  "dependencies": {
    "@types/ws": "^7.2.1",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "rxjs": "^6.5.4",
    "socks": "^2.4.4",
    "ts-custom-error": "^3.1.1",
    "ts-xor": "^1.0.8",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/jest": "^25.1.2",
    "@types/lodash": "^4.14.149",
    "@types/node": "^13.7.0",
    "@typescript-eslint/eslint-plugin": "^2.19.1",
    "@typescript-eslint/parser": "^2.19.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "keywords": [
    "MQTT",
    "subscribe",
    "iot",
    "tls"
  ],
  "author": "nerixyz",
  "license": "MIT"
}
